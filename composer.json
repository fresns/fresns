{
    "name": "fresns/fresns",
    "type": "project",
    "description": "Cross-platform general-purpose multiple content forms social network service software.",
    "keywords": [
        "fresns",
        "sns",
        "social",
        "club",
        "group",
        "bbs",
        "forum",
        "community",
        "communities"
    ],
    "license": "Apache-2.0",
    "homepage": "https://fresns.org",
    "support": {
        "issues": "https://github.com/fresns/fresns/issues",
        "source": "https://github.com/fresns/fresns",
        "docs": "https://fresns.org"
    },
    "authors": [
        {
            "name": "Jevan Tang",
            "email": "jevan@fresns.org",
            "homepage": "https://github.com/jevantang",
            "role": "Creator"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/scout": "^9.8",
        "laravel/ui": "^4.2",
        "doctrine/dbal": "^3.6",
        "fresns/dto": "^1.1",
        "fresns/cmd-word-manager": "^1.3",
        "fresns/plugin-manager": "^2.6",
        "fresns/theme-manager": "^2.1",
        "fresns/market-manager": "^2.1",
        "mcamara/laravel-localization": "^1.8",
        "hisorange/browser-detect": "^4.5",
        "rap2hpoutre/fast-excel": "^5.1",
        "xantios/mimey": "^2.2",
        "nelexa/zip": "^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laravel/pint": "^1.5",
        "laravel/sail": "^1.20",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "extensions/plugins/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": true,
            "merge-extra-deep": true
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": false,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
