{
    "name": "fresns/fresns",
    "type": "project",
    "description": "Cross-platform general-purpose multiple content forms social network service software.",
    "keywords": [
        "fresns",
        "sns",
        "social",
        "club",
        "group",
        "bbs",
        "forum",
        "community",
        "communities"
    ],
    "license": "Apache-2.0",
    "homepage": "https://fresns.org",
    "support": {
        "issues": "https://github.com/fresns/fresns/issues",
        "source": "https://github.com/fresns/fresns",
        "docs": "https://fresns.org"
    },
    "authors": [
        {
            "name": "Jarvis Tang",
            "email": "jarvis.okay@gmail.com",
            "homepage": "https://github.com/jarvis-tang",
            "role": "Creator"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.41",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.1",
        "fresns/dto": "^1.1",
        "fresns/cmd-word-manager": "^1.2",
        "fresns/plugin-manager": "^2.1",
        "fresns/theme-manager": "^2.0",
        "fresns/market-manager": "^2.1",
        "mcamara/laravel-localization": "^1.7",
        "hisorange/browser-detect": "^4.5",
        "maatwebsite/excel": "^3.1",
        "xantios/mimey": "^2.2",
        "nelexa/zip": "^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "extensions/plugins/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": true,
            "merge-extra-deep": true
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": false,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
